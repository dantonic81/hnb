{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "sku": {
      "type": "integer",
      "unique": true
    },
    "name": {
      "type": "string"
    },
    "price": {
      "type": "string",
      "pattern": "^\\d+(\\.\\d{1,2})?$"
    },
    "category": {
      "type": "string"
    },
    "popularity": {
      "type": "number",
      "minimum": 0
    }
  },
  "required": ["sku", "name", "price", "category", "popularity"],
  "additionalProperties": false
}


